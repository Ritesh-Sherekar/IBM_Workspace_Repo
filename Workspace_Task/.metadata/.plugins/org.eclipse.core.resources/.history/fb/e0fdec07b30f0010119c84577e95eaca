<?xml version="1.0" encoding="UTF-8"?>
<wsdl:definitions xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
                  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" 
                  xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
                  xmlns:tns="com.ghipss.gip" 
                  targetNamespace="com.ghipss.gip">
    
    <wsdl:types>
        <xsd:schema targetNamespace="com.ghipss.gip">
            <xsd:element name="GIPTransactionOpRequest">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="Amount" type="xsd:string"/>
                        <xsd:element name="datetime" type="xsd:string"/>
                        <xsd:element name="TrackingNum" type="xsd:string"/>
                        <xsd:element name="FunctionCode" type="xsd:string"/>
                        <xsd:element name="OrigineBank" type="xsd:string"/>
                        <xsd:element name="DestBank" type="xsd:string"/>
                        <xsd:element name="SessionID" type="xsd:string"/>
                        <xsd:element name="ChannelCode" type="xsd:string"/>
                        <xsd:element name="AccountToDebit" type="xsd:string"/>
                        <xsd:element name="NameToDebit" type="xsd:string"/>
                        <xsd:element name="AccountToCredit" type="xsd:string"/>
                        <xsd:element name="NameToCredit" type="xsd:string"/>
                        <xsd:element name="Narration" type="xsd:string"/>
                        <xsd:element name="Login" type="xsd:string"/>
                        <xsd:element name="Password" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="GIPTransactionOpResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="Amount" type="xsd:string"/>
                        <xsd:element name="datetime" type="xsd:string"/>
                        <xsd:element name="TrackingNum" type="xsd:string"/>
                        <xsd:element name="FunctionCode" type="xsd:string"/>
                        <xsd:element name="OrigineBank" type="xsd:string"/>
                        <xsd:element name="DestBank" type="xsd:string"/>
                        <xsd:element name="SessionID" type="xsd:string"/>
                        <xsd:element name="NameToDebit" type="xsd:string"/>
                        <xsd:element name="AccountToDebit" type="xsd:string"/>
                        <xsd:element name="NameToCredit" type="xsd:string"/>
                        <xsd:element name="AccountToCredit" type="xsd:string"/>
                        <xsd:element name="Narration" type="xsd:string"/>
                        <xsd:element name="OneTimePassword" type="xsd:string" nillable="true"/>
                        <xsd:element name="Login" type="xsd:string" nillable="true"/>
                        <xsd:element name="Password" type="xsd:string" nillable="true"/>
                        <xsd:element name="ActCode" type="xsd:string"/>
                        <xsd:element name="AprvCode" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:schema>
    </wsdl:types>

    <wsdl:message name="GIPTransactionOpRequestMessage">
        <wsdl:part name="parameters" element="tns:GIPTransactionOpRequest"/>
    </wsdl:message>
    
    <wsdl:message name="GIPTransactionOpResponseMessage">
        <wsdl:part name="parameters" element="tns:GIPTransactionOpResponse"/>
    </wsdl:message>
    
    <wsdl:portType name="GIPTransactionOpPortType">
        <wsdl:operation name="GIPTransactionOp">
            <wsdl:input message="tns:GIPTransactionOpRequestMessage"/>
            <wsdl:output message="tns:GIPTransactionOpResponseMessage"/>
        </wsdl:operation>
    </wsdl:portType>
    
    <wsdl:binding name="GIPTransactionOpBinding" type="tns:GIPTransactionOpPortType">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <wsdl:operation name="GIPTransactionOp">
            <soap:operation soapAction="com.ghipss.gip/GIPTransactionOp"/>
            <wsdl:input>
                <soap:body use="literal"/>
            </wsdl:input>
            <wsdl:output>
                <soap:body use="literal"/>
            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>
    
    <wsdl:service name="GIPTransactionOpService">
        <wsdl:port name="GIPTransactionOpPort" binding="tns:GIPTransactionOpBinding">
            <soap:address location="http://example.com/GIPTransactionOp"/>
        </wsdl:port>
    </wsdl:service>
</wsdl:definitions>