		DECLARE response CHARACTER;
		SET response = '<GIPTransactionResponse>
		                <Amount>000000000000</Amount>
		                <datetime>2306301811</datetime>
		                <TrackingNum>125278</TrackingNum>
		                <FunctionCode>230</FunctionCode>
