

CREATE COMPUTE MODULE Procedures_flow_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		-- CALL CopyMessageHeaders();
	   -- CALL CopyEntireMessage();
	    
	    DECLARE ABC REFERENCE TO InputRoot.XMLNSC.EMPS;
	    CALL ADDDATA(ABC.ID,ABC.NAME);
	    
		RETURN TRUE;
	END;

	

CREATE PROCEDURE ADDDATA (IN ID VARCHAR2,IN NAME VARCHAR2)
LANGUAGE DATABASE
EXTERNAL NAME  "NEWPROCEDURES";
